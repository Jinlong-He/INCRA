MODULE main
IVAR
c : 0..127;
VAR
R0 : integer;
s_0_0 : {v0,t,i};
R0_l : boolean;
INIT
s_0_0 = i
INIT
R0 = 0
ASSIGN
next(R0) := case
( c >= 0 & c <= 127 & s_0_0 = v0 & ( R0_l = TRUE ) ) : R0 + 1;
TRUE : R0;
esac;
next(s_0_0) := case
( !TRUE & s_0_0 = i ) : s_0_0;
( TRUE & s_0_0 = i ) : v0;
( ( c >= 0 & c <= 127 & s_0_0 = v0 ) & ( R0_l = TRUE ) ) : v0;
TRUE : t;
esac;
VAR
INVARSPEC !(( ( s_0_0 = v0 ) ) & TRUE & ((R0 + -1 * 5) = 0) & TRUE)